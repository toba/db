      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.6.11",
      "@toba/test": "^4.0.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/db"
   },
   "dependencies": {
      "@toba/tools": "^6.7.5",
      "ulid": "^2.3.0"
   }
